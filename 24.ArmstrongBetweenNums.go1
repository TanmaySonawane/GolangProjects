package main

import (
	"fmt"
	"math"
)

func main() {
	var a int = 10
	var b int = 99000
	for i := a; i <= b; i++ {
		digits := 0
		sum := 0
		var c int = i
		for c > 0 {
			digits++
			c /= 10
		}
		c = i
		for c > 0 {
			sum += int(math.Pow(float64(c%10), float64(digits)))
			c /= 10
		}
		if i == sum {
			fmt.Println(i)
		}
	}
}
